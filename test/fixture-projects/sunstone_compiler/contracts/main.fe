use foo::{Foo, Bar, FooBar}
use std::context::Context

contract FooBarBing {
    pub fn get_bar() -> Bar {
        return Bar(
            my_u256: 24
        )
    }

    pub fn create_foobar_contract(ctx: Context) -> u256 {
        let foo_bar: FooBar = FooBar.create(ctx, 0)
        return foo_bar.add(x: 40, y: 50)
    }
}
